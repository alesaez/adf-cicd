{
    "$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "factoryName": {
            "type": "string",
            "metadata": "Data Factory name",
            "defaultValue": "asa-NPRD-dev-df"
        },
        "AzureSqlDatabase_password": {
            "type": "secureString",
            "metadata": "Secure string for 'password' of 'AzureSqlDatabase'"
        },
        "AzureDataLakeStorage_properties_typeProperties_url": {
            "type": "string",
            "defaultValue": "https://stasanprd.dfs.core.windows.net/"
        },
        "AzureSqlDatabase_properties_typeProperties_server": {
            "type": "string",
            "defaultValue": "asa-nprd-sql.database.windows.net"
        },
        "AzureSqlDatabase_properties_typeProperties_database": {
            "type": "string",
            "defaultValue": "AdventureWorksDB"
        },
        "AzureSqlDatabase_properties_typeProperties_userName": {
            "type": "string",
            "defaultValue": "sqladminuser"
        },
        "containerUri": {
            "type": "string",
            "metadata": "URI to storage account containing linked ARM templates"
        },
        "containerSasToken": {
            "type": "string",
            "metadata": "Sas token to storage account containing linked ARM templates"
        }
    },
    "variables": {
        "factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
    },
    "resources": [
        {
            "name": "asa-NPRD-dev-df_ArmTemplate_0",
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2024-03-01",
            "properties": {
                "mode": "incremental",
                "templateLink": {
                    "uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "factoryName": {
                        "value": "[parameters('factoryName')]"
                    },
                    "AzureSqlDatabase_password": {
                        "value": "[parameters('AzureSqlDatabase_password')]"
                    },
                    "AzureDataLakeStorage_properties_typeProperties_url": {
                        "value": "[parameters('AzureDataLakeStorage_properties_typeProperties_url')]"
                    },
                    "AzureSqlDatabase_properties_typeProperties_server": {
                        "value": "[parameters('AzureSqlDatabase_properties_typeProperties_server')]"
                    },
                    "AzureSqlDatabase_properties_typeProperties_database": {
                        "value": "[parameters('AzureSqlDatabase_properties_typeProperties_database')]"
                    },
                    "AzureSqlDatabase_properties_typeProperties_userName": {
                        "value": "[parameters('AzureSqlDatabase_properties_typeProperties_userName')]"
                    }
                }
            },
            "dependsOn": []
        }
    ]
}